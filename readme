# Generate a new API key
curl -X POST \
  -H "Content-Type: application/json" \
  -d '{"description": "Development key"}' \
  http://localhost:3000/api/keys

# List all keys for a user
curl http://localhost:3000/api/keys/user123

# Revoke an API key
curl -X DELETE http://localhost:3000/api/keys/your-api-key

# Update key description
curl -X PATCH \
  -H "Content-Type: application/json" \
  -d '{"description": "Updated description"}' \
  http://localhost:3000/api/keys/your-api-key